<p id="notice"><%= notice %></p>

<h1>ユーザー一覧</h1>

<table>
  <thead>
  <tr>
    <th><%= User.human_attribute_name(:name) %></th>
    <th><%= User.human_attribute_name(:self_introduction) %></th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.self_introduction %></td>
      <td><%= link_to '詳細' ,user %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>
<%= paginate @users %>
<%= link_to "Back", :back %>